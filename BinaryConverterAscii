import java.util.Scanner;

class MyProgram {
    public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    
    String binary = "";
    String text = "";
    String code = "";
    
        System.out.println("Enter some text: ");
        
        text = sc.nextLine();
        
        System.out.println("The printed out binary sequence is: ");
        
        binaryText (binary, text, code);
        
    }
    
    public static void binaryText (String bin, String txt, String cd){
        
        
        for (int i = 0; i < txt.length(); i++){
            
        if (txt.charAt(i) == ' '){
            bin = "0100000 ";
        }
        if (txt.charAt(i) == '!'){
            bin = "0100001 ";
        }
        if (txt.charAt(i) == '"'){
            bin = "0100010 ";
        }
        if (txt.charAt(i) == '#'){
            bin = "0100011 ";
        }
        if (txt.charAt(i) == '$'){
            bin = "0100100 ";
        }
        if (txt.charAt(i) == '%'){
            bin = "0100101 ";
        }
        if (txt.charAt(i) == '&'){
            bin = "0100110 ";
        }
        if (txt.charAt(i) == '\''){
            bin = "0100111 ";
        }
        if (txt.charAt(i) == '('){
            bin = "0101000 ";
        }
        if (txt.charAt(i) == ')'){
            bin = "0101001 ";
        }
        if (txt.charAt(i) == '*'){
            bin = "0101010 ";
        }
        if (txt.charAt(i) == '+'){
            bin = "0101011 ";
        }
        if (txt.charAt(i) == ','){
            bin = "0101100 ";
        }
        if (txt.charAt(i) == '-'){
            bin = "0101101 ";
        }
        if (txt.charAt(i) == '.'){
            bin = "0101110 ";
        }
        if (txt.charAt(i) == '/'){
            bin = "0101111 ";
        }
        if (txt.charAt(i) == '0'){
            bin = "0110000 ";
        }
        if (txt.charAt(i) == '1'){
            bin = "0110001 ";
        }
        if (txt.charAt(i) == '2'){
            bin = "0110010 ";
        }
        if (txt.charAt(i) == '3'){
            bin = "0110011 ";
        }
        if (txt.charAt(i) == '4'){
            bin = "0110100 ";
        }
        if (txt.charAt(i) == '5'){
            bin = "0110101 ";
        }
        if (txt.charAt(i) == '6'){
            bin = "0110110 ";
        }
        if (txt.charAt(i) == '7'){
            bin = "0110111 ";
        }
        if (txt.charAt(i) == '8'){
            bin = "0111000 ";
        }
        if (txt.charAt(i) == '9'){
            bin = "0111001 ";
        }
        if (txt.charAt(i) == ':'){
            bin = "0111010 ";
        }
        if (txt.charAt(i) == ';'){
            bin = "0111011 ";
        }
        if (txt.charAt(i) == '<'){
            bin = "0111100 ";
        }
        if (txt.charAt(i) == '='){
            bin = "0111100 ";
        }
        if (txt.charAt(i) == '>'){
            bin = "0111110 ";
        }
        if (txt.charAt(i) == '?'){
            bin = "0111111 ";
        }
        if (txt.charAt(i) == '@'){
            bin = "1000000 ";
        }
        
            
        if (txt.charAt(i) == 'A'){
            bin = "1000001 ";
        }
        if (txt.charAt(i) == 'B'){
            bin = "1000010 ";
        }
        if (txt.charAt(i) == 'C'){
            bin = "1000011 ";
        }
        if (txt.charAt(i) == 'D'){
            bin = "1000100 ";
        }
        if (txt.charAt(i) == 'E'){
            bin = "1000101 ";
