        if (txt.charAt(i) == 'W'){
            bin = "1010111 ";
        }
        if (txt.charAt(i) == 'X'){
            bin = "1011000 ";
        }
        if (txt.charAt(i) == 'Y'){
            bin = "1011001 ";
        }
        if (txt.charAt(i) == 'Z'){
            bin = "1011010 ";
        }
        if (txt.charAt(i) == '['){
            bin = "1011011 ";
        }
        if (txt.charAt(i) == ']'){
            bin = "1011101 ";
        }
        if (txt.charAt(i) == '^'){
            bin = "1011110 ";
        }
        if (txt.charAt(i) == '_'){
            bin = "1011111 ";
        }
        if (txt.charAt(i) == '`'){
            bin = "1100000 ";
        }
        
        if (txt.charAt(i) == 'a'){
            bin = "1100001 ";
        }
        if (txt.charAt(i) == 'b'){
            bin = "1100010 ";
        }
        if (txt.charAt(i) == 'c'){
            bin = "1100011 ";
        }
        if (txt.charAt(i) == 'd'){
            bin = "1100100 ";
        }
        if (txt.charAt(i) == 'e'){
            bin = "1100101 ";
        }
        if (txt.charAt(i) == 'f'){
            bin = "1100110 ";
        }
        if (txt.charAt(i) == 'g'){
            bin = "1100111 ";
        }
        if (txt.charAt(i) == 'h'){
            bin = "1101000 ";
        }
        if (txt.charAt(i) == 'i'){
            bin = "1101001 ";
        }
        if (txt.charAt(i) == 'j'){
            bin = "1101010 ";
        }
        if (txt.charAt(i) == 'k'){
            bin = "1101011 ";
        }
        if (txt.charAt(i) == 'l'){
            bin = "1101100 ";
        }
        if (txt.charAt(i) == 'm'){
            bin = "1101101 ";
        }
        if (txt.charAt(i) == 'n'){
            bin = "1101110 ";
        }
        if (txt.charAt(i) == 'o'){
            bin = "1101111 ";
        }
        if (txt.charAt(i) == 'p'){
            bin = "1110000 ";
        }
        if (txt.charAt(i) == 'q'){
            bin = "1110001 ";
        }
        if (txt.charAt(i) == 'r'){
            bin = "1110010 ";
        }
        if (txt.charAt(i) == 's'){
            bin = "1110011 ";
        }
        if (txt.charAt(i) == 't'){
            bin = "1110100 ";
        }
        if (txt.charAt(i) == 'u'){
            bin = "1110101 ";
        }
        if (txt.charAt(i) == 'v'){
            bin = "1110110 ";
        }
        if (txt.charAt(i) == 'w'){
            bin = "1110111 ";
        }
        if (txt.charAt(i) == 'x'){
            bin = "1111000 ";
        }
        if (txt.charAt(i) == 'y'){
            bin = "1111001 ";
        }
        if (txt.charAt(i) == 'z'){
            bin = "1111010 ";
        }
        if (txt.charAt(i) == '{'){
            bin = "1111011 ";
        }
        if (txt.charAt(i) == '|'){
            bin = "1111100 ";
        }
        if (txt.charAt(i) == '}'){
            bin = "1111101 ";
        }
        if (txt.charAt(i) == '~'){
            bin = "1111110 ";
        }
        
        cd = cd + bin;
        
        
    }
    
    System.out.println(cd);
    
    }
}