        if (txt.charAt(i) == 'K'){
            bin = "1001011 ";
        }
        if (txt.charAt(i) == 'L'){
            bin = "1001100 ";
        }
        if (txt.charAt(i) == 'M'){
            bin = "1001101 ";
        }
        if (txt.charAt(i) == 'N'){
            bin = "1001110 ";
        }
        if (txt.charAt(i) == 'O'){
            bin = "1001111 ";
        }
        if (txt.charAt(i) == 'P'){
            bin = "1010000 ";
        }
        if (txt.charAt(i) == 'Q'){
            bin = "1010001 ";
        }
        if (txt.charAt(i) == 'R'){
            bin = "1010010 ";
        }
        if (txt.charAt(i) == 'S'){
            bin = "1010011 ";
